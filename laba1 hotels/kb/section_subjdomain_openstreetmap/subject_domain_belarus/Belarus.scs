//openstreetmap tags
name_en <-concept_openstreetmap_tag;
=>nrel_main_idtf:
    [name:en]
    (* <-lang_en;; *);;
name_ru <-concept_openstreetmap_tag;
=>nrel_main_idtf:
    [name:ru]
    (* <-lang_en;; *);;
admin_level <- concept_openstreetmap_tag;
=>nrel_main_idtf:
    [admin_level]
    (*<-lang_en;;*);
<=nrel_tag: nrel_admin_level;;
addr_city <- concept_openstreetmap_tag;
=>nrel_main_idtf:
    [addr:city]
    (*<-lang_en;;*);
<=nrel_tag: nrel_city;;
addr_district <- concept_openstreetmap_tag;
=>nrel_main_idtf:
    [addr:district]
    (*<-lang_en;;*);
<=nrel_tag: nrel_district;;
addr_region <- concept_openstreetmap_tag;
=>nrel_main_idtf:
    [addr:region]
    (*<-lang_en;;*);
<=nrel_tag: nrel_region;;
addr_country <- concept_openstreetmap_tag;
=>nrel_main_idtf:
    [addr:country]
    (*<-lang_en;;*);
<=nrel_tag: nrel_country;;


// belarus description
belarus <- concept_country;
=>nrel_main_idtf:
    [Belarus]
    (* <- lang_en;; <- name_en;;*);
    [Беларусь]
    (* <- lang_ru;; <- name_ru;;*);
    [BY]
    (* <- name;; *);
=>nrel_admin_level:
    [2]
    (*<-concept_admin_level;;*);
=>nrel_capital:
    minsk;;


